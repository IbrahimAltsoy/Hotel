@model InstagramFollowersDto


<div class="col-lg-3 col-sm-6">
    <div class="card">
        <div class="social-graph-wrapper widget-googleplus">
            <span class="s-icon"><i class="fa fa-google-plus"></i></span>
        </div>
        <div class="row">
            <div class="col-6 border-right">
                <div class="pt-3 pb-3 pl-0 pr-0 text-center">
                    <h4 class="m-1">@* @Model.following *@ 2190</h4>
                    <p class="m-0">Arkadaşlarım</p>
                </div>
            </div>
            <div class="col-6">
                <div class="pt-3 pb-3 pl-0 pr-0 text-center">
                    <h4 class="m-1">@* @Model.followers *@ 455</h4>
                    <p class="m-0">Takipçi Sayısı</p>
                </div>
            </div>
        </div>
    </div>
</div>




